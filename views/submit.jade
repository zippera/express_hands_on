extends layout

block content
    h1 Submit
    form(method='post')
        div
            label Title
            input(type='text', name='news_title')
        div
            label URL
            input(type='text', name='news_url')
        div 
            label Text
            input(type='textarea', name='news_text')
        div
            input(type='submit', value='submit')
